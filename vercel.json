{
  "version": 2,
  "builds": [
    {
      "src": "api/*.php",
      "use": "vercel-php@0.9.0"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/totp",
      "dest": "/api/totp.php"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}